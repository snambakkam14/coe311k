    "import numpy as np\n",
    "\n",
    "def base_two(x):\n",
    "    bits = []\n",
    "    while(x>0):\n",
    "        bits.append(x%2)\n",
    "        x = x//2\n",
    "        \n",
    "    return np.array(bits[::-1])\n",
    "\n",
    "base_two(467)"
